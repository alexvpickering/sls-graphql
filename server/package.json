{
  "name": "serverless-graphql",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:serverless/serverless-graphql.git"
  },
  "scripts": {
    "test": "jest --watchAll",
    "dbkill": "kill $(lsof -t -i:8000) && kill $(lsof -t -i:4000) || true",
    "lstack": "SERVICES=s3:4572 localstack start",
    "s3seed": "node seed-data/s3seed.js",
    "start": "yarn s3seed && yarn dbkill && serverless dynamodb install && env-cmd ../config/security.env.local serverless offline start",
    "deploy-prod": "env-cmd ../config/security.env.prod serverless --stage=production deploy"
  },
  "dependencies": {
    "apollo-server-lambda": "1.3.2",
    "apollo-tracing": "^0.0.9",
    "aws-xray-sdk": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "graphql": "^0.10.5",
    "graphql-playground-middleware-lambda": "^1.2.0",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "2.7.2",
    "serverless-dynamodb-client": "^0.0.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "aws-sdk": "^2.216.1",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.15.2",
    "cors": "2.8.4",
    "env-cmd": "^5.0.0",
    "faker": "^4.1.0",
    "imports-loader": "^0.7.1",
    "jest": "^21.2.1",
    "jest-cli": "^22.4.3",
    "jsonfile": "^4.0.0",
    "serverless-dynamodb-local": "^0.2.26",
    "serverless-offline": "^3.15.3",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^4.1.0",
    "webpack": "^3.8.1",
    "webpack-node-externals": "^1.6.0"
  }
}
